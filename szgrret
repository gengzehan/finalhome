#include <stdio.h>
#include <stdlib.h>
double choice(char way,double a,double b)
{
     double re;
     switch(way)
     {
         case '+':re=a+b;break;
         case '-':re=a-b;break;
         case '*':re=a*b;break;
         case '/':re=a/b;break;
     }
     return re;
}
int main()
{
    double num[8],re,fi=0;
    char ch[8];
    int i=0,hp=0,flag=0,hp2;
    scanf("%lf",&num[i]);
    while((ch[i]=getchar())!='\n'&&(flag=i+1))
        scanf("%lf",&num[++i]);
    while(ch[hp]!='\n')
    {
        if(ch[hp]=='*'||ch[hp]=='/')
        {
            re=choice(ch[hp],num[hp],num[hp+1]);
            num[hp]=re;
            hp2=hp;
            while(ch[hp2]!='\n')
            {
                num[hp2+1]=num[hp2+2];
                ch[hp2]=ch[hp2+1];
                hp2++;
            }
            continue;
        }
        hp++;
    }
    hp=0;
    while(ch[hp]!='\n')
    {
        fi=choice(ch[hp],num[hp],num[hp+1]);
        num[hp+1]=fi;
        hp++;
    }
    if(flag==0)
        fi=num[0];
    printf("%.3f",fi);
}
